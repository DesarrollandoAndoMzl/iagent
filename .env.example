# Google Gemini API Key
# Obtén tu clave en: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# PostgreSQL connection string
# Ejemplo: postgresql://user:password@localhost:5432/voice_agent_db
DATABASE_URL=postgresql://user:password@localhost:5432/voice_agent_db

# Puerto del servidor
PORT=8080

# Orígenes permitidos para CORS (separados por coma, o * para todos)
ALLOWED_ORIGINS=*
